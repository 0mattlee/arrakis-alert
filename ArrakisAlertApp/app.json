{
  "expo": {
    "name": "ArrakisAlertApp",
    "slug": "arrakis-alert-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.arrakisalertapp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.yourcompany.arrakisalertapp",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dfcc76b7-e388-4642-b959-8f84a4be7246"
      },
      "backendApiUrl": "https://arrakis-alert.onrender.com/api"
    },
    "owner": "0mattlee"
  }
}
